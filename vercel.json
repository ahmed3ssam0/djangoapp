{
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "functions": {
    "api/*.py": {
      "runtime": "vercel-python@0.5.0"
    }
  },
  "github": {
    "silent": false
  },
  "installCommand": "pip3 install -r requirements.txt"
}
